<div class="dashboard">
  <h2>Welcome Back Nehanth!</h2>

  <a href="/add-question">+ Add New Question</a>

  <ul>
    <% data.forEach(element => { %>
      <li>
        <a href="/question-view/<%= element._id %>"><%= element.question %></a>
        <a href="/edit-question/<%= element._id %>">Edit</a>
        <form action="/delete-question/<%= element._id %>" method="POST">
          <input type="submit" value="Delete Question">
        </form>
      </li>
    <% }) %>
  </ul>
</div>
